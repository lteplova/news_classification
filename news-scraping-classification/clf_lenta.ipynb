        "# препроцессинг текста\n",
        "\n",
        "# mystopwords = stopwords.words('russian') + ['фото', 'фонтанка', 'риа', 'ру',\n",
        "#     'это', 'наш' , 'тыс', 'млн', 'млрд', 'также',  'т', 'д',\n",
