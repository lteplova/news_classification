        "# смотрим распределение категорий\n",
        "def loads_labels(dataset):\n",
        "    labels = []\n",
        "    for i in dataset:\n",
        "        labels.append(i)\n",
        "    counts = Counter(labels)\n",
        "    count_df =  pd.DataFrame([(k, v) for k, v in counts.items()],\n",
        "    columns=['category', 'count'])\n",
        "    return count_df.sort_values(by = 'count', ascending = False)"
      ],
